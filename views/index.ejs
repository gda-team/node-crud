<ul class="list-group">
	<li class="list-group-item bg-danger text-light">Mes contacts</li>
	<% contacts && contacts.forEach(contact => { %>
	<li class="list-group-item d-flex justify-content-between">
		<h5><%= contact.nom %> <%= contact.prenom %></h5>
		<a class="btn btn-danger btn-sm" href="/<%= contact.id %>">Voir plus</a>
	</li>
	<% }) %>
</ul>

